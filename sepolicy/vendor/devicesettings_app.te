# Allow devicesettings_app to read and write sysfs nodes

allow devicesettings_app vendor_sysfs_graphics:dir search;
allow devicesettings_app vendor_sysfs_graphics:file read;
allow devicesettings_app vendor_sysfs_graphics:file write;
allow devicesettings_app vendor_sysfs_graphics:file open;
allow devicesettings_app vendor_sysfs_graphics:file getattr;
